<center><h1> ($) Messages (/$) </h1></center>

<body>

	<table class="table table-hover">
  		<thead>
    			<tr>
      				<th>Name</th>
      				<th>Phone Number</th>
      				<th>Message</th>
      				<th>Date</th>
      				<th>Time</th>
    			</tr>
  		</thead>

  		<tbody>
    		<% @messages.each do |m| %>
      			<tr>
        			<td><%= m.name %></td>
        			<td><%= m.phonenumber %></td>
        			<td><%= m.message %></td>
        			<td><%= m.date %></td>
        			<td><%= m.time %></td>
      			</tr>
    	<% end %>
  		</tbody>
	</table>

</body>

