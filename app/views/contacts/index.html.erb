<center><h1> ($) Contacts (/$) </h1></center>

<body>
	
	<table class="table table-hover">
  		<thead>
    			<tr>
      				<th>Name</th>
      				<th>Phone Number</th>
    			</tr>
  		</thead>

  		<tbody>
    		<% @contacts.each do |c| %>
      			<tr>
        			<td><%= c.name %></td>
        			<td><%= c.phonenumber %></td>
      			</tr>
    	<% end %>
  		</tbody>
	</table>

